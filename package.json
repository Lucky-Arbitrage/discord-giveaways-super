{
  "name": "@jsewell/discord-giveaways-super",
  "version": "1.1.2",
  "description": "Fork of discord giveaways super for LA",
  "types": "./types/src/index.d.ts",
  "main": "dist/src/index.js",
	"directories": {
		"lib": "./src"
	},
"scripts": {
		"prep": "npx husky install && npm run commitlint:install",
